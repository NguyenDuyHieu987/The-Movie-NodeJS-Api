{
  "name": "the-movie-nodejs-api",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "start": "set NODE_ENV=development && cls && nodemon src/index.ts",
    "start:prod": "cross-env NODE_ENV=production && rimraf ./build && tsc && node build/index.js",
    "start:pm2": "cross-env NODE_ENV=production && rimraf ./build && tsc && pm2 start ecosystem.config.cjs",
    "build": "cross-env NODE_ENV=production && rimraf ./build && tsc && node --max-old-space-size=4096 node_modules/.bin/tsc",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "typecheck:once": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint --cache src/**/*.{ts,cts,js,cjs}",
    "format": "eslint --fix --cache src/**/*.{ts,cts,js,cjs}",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sendinblue/client": "3.3.1",
    "argon2": "0.43.0",
    "body-parser": "2.2.0",
    "compression": "1.8.0",
    "connect-mongo": "5.1.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "crypto": "1.0.1",
    "crypto-js": "4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "16.5.0",
    "express": "4.21.2",
    "express-rate-limit": "7.5.0",
    "express-session": "1.18.1",
    "googleapis": "149.0.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.1.2",
    "http-errors": "2.0.0",
    "iso-3166-1": "^2.1.1",
    "iso-639-1": "^3.1.5",
    "jsonwebtoken": "9.0.2",
    "mathjs": "^14.5.0",
    "module-alias": "2.2.3",
    "moment": "2.30.1",
    "mongoose": "8.15.0",
    "multer": "2.0.0",
    "node-fetch": "2.7.0",
    "qs": "6.14.0",
    "redis": "5.1.0",
    "serve-favicon": "^2.5.0",
    "sib-api-v3-sdk": "8.5.0",
    "socket.io": "^4.8.1",
    "std-env": "3.9.0",
    "stripe": "18.1.1",
    "tsconfig-paths": "4.2.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.8.0",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.18",
    "@types/crypto-js": "4.2.2",
    "@types/express": "4.17.21",
    "@types/express-session": "1.18.1",
    "@types/jsonwebtoken": "9.0.7",
    "@types/module-alias": "2.0.4",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.10",
    "@types/node-fetch": "2.6.12",
    "@types/request": "2.48.12",
    "@types/serve-favicon": "^2.5.7",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-n": "17.18.0",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "16.0.0",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18.x"
  }
}
