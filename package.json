{
  "name": "the-movie-nodejs-api",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "start": "set NODE_ENV=development && cls && nodemon src/index.ts",
    "start:prod": "cross-env NODE_ENV=production && rimraf ./build && tsc && node build/index.js",
    "start:pm2": "cross-env NODE_ENV=production && rimraf ./build && tsc && pm2 start ecosystem.config.cjs",
    "build": "cross-env NODE_ENV=production && rimraf ./build && tsc",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "typecheck:once": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint --cache src/**/*.{ts,cts,js,cjs}",
    "format": "eslint --fix --cache src/**/*.{ts,cts,js,cjs}",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sendinblue/client": "3.3.1",
    "argon2": "0.41.0",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "connect-mongo": "5.1.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "crypto": "1.0.1",
    "crypto-js": "4.2.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "7.4.0",
    "express-session": "1.18.0",
    "googleapis": "143.0.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "module-alias": "2.2.3",
    "moment": "2.30.1",
    "mongoose": "8.6.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "2.7.0",
    "qs": "6.13.0",
    "redis": "4.7.0",
    "serve-favicon": "^2.5.0",
    "sib-api-v3-sdk": "8.5.0",
    "std-env": "3.7.0",
    "stripe": "16.9.0",
    "tsconfig-paths": "4.2.0",
    "uuid": "10.0.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/crypto-js": "4.2.2",
    "@types/express": "4.17.21",
    "@types/express-session": "1.18.0",
    "@types/jsonwebtoken": "9.0.6",
    "@types/module-alias": "2.0.4",
    "@types/multer": "1.4.12",
    "@types/node": "22.5.1",
    "@types/node-fetch": "2.6.11",
    "@types/request": "2.48.12",
    "@types/serve-favicon": "^2.5.7",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.5.12",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.8.1",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.5",
    "jest": "29.7.0",
    "lint-staged": "15.2.9",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18.x"
  }
}
