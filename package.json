{
  "name": "the-movie-nodejs-api",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "start": "set NODE_ENV=development && cls && nodemon src/index.ts",
    "start:prod": "cross-env NODE_ENV=production && rimraf ./build && tsc && node build/index.js",
    "start:pm2": "cross-env NODE_ENV=production && rimraf ./build && tsc && pm2 start ecosystem.config.cjs",
    "build": "cross-env NODE_ENV=production && rimraf ./build && node --max-old-space-size=4096 node_modules/.bin/tsc",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "typecheck:once": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint --cache src/**/*.{ts,cts,js,cjs}",
    "format": "eslint --fix --cache src/**/*.{ts,cts,js,cjs}",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sendinblue/client": "3.3.1",
    "argon2": "0.44.0",
    "body-parser": "2.2.0",
    "chalk": "^5.6.0",
    "compression": "1.8.1",
    "connect-mongo": "5.1.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "cross-env": "10.0.0",
    "crypto": "1.0.1",
    "crypto-js": "4.2.0",
    "dayjs": "^1.11.18",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "express-session": "1.18.2",
    "googleapis": "159.0.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "http-errors": "2.0.0",
    "iso-3166-1": "^2.1.1",
    "iso-639-1": "^3.1.5",
    "jsonwebtoken": "9.0.2",
    "mathjs": "^14.6.0",
    "module-alias": "2.2.3",
    "moment": "2.30.1",
    "mongoose": "8.18.0",
    "multer": "2.0.2",
    "node-fetch": "3.3.2",
    "qs": "6.14.0",
    "redis": "5.8.2",
    "serve-favicon": "^2.5.1",
    "sib-api-v3-sdk": "8.5.0",
    "socket.io": "^4.8.1",
    "stripe": "18.5.0",
    "tsconfig-paths": "4.2.0",
    "uuid": "12.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.6",
    "@types/compression": "1.8.1",
    "@types/cookie-parser": "1.4.9",
    "@types/cors": "2.8.19",
    "@types/crypto-js": "4.2.2",
    "@types/express": "5.0.3",
    "@types/express-session": "1.18.2",
    "@types/jsonwebtoken": "9.0.7",
    "@types/module-alias": "2.0.4",
    "@types/multer": "2.0.0",
    "@types/node": "24.3.1",
    "@types/node-fetch": "2.6.13",
    "@types/request": "2.48.13",
    "@types/serve-favicon": "^2.5.7",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.2.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "lint-staged": "16.1.6",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18.x"
  }
}
