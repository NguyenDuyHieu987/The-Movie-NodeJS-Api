{
  "name": "the-movie-nodejs-api",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "start": "set NODE_ENV=development && cls && nodemon src/index.ts",
    "start:prod": "cross-env NODE_ENV=production && rimraf ./build && tsc && node build/index.js",
    "start:pm2": "cross-env NODE_ENV=production && rimraf ./build && tsc && pm2 start ecosystem.config.cjs",
    "build": "cross-env NODE_ENV=production && rimraf ./build && tsc",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "typecheck:once": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint --cache src/**/*.{ts,cts,js,cjs}",
    "format": "eslint --fix --cache src/**/*.{ts,cts,js,cjs}",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sendinblue/client": "^3.3.1",
    "argon2": "^0.40.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "googleapis": "^134.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "qs": "^6.12.0",
    "redis": "^4.6.13",
    "sib-api-v3-sdk": "^8.5.0",
    "std-env": "^3.7.0",
    "stripe": "^14.22.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/request": "^2.48.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
